{
  "hosting": {
    "public": "_site",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "/service-worker.js",
      "headers": [ {
          "key": "Cache-Control",
          "value": "no-cache,no-store,must-revalidate"
        } ]
    } ]
  }
}
